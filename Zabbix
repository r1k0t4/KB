#Increase log sverity
zabbix_agentd --runtime-control log_level_increase

#Grep remote command [KH 110305]
tailf /var/log/zabbix/zabbix_agentd.log | grep -C5 system.run

tailf /var/log/messages | grep 'comm="sh"'

