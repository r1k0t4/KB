URL=https://github.com/bomyst/documentacion.git

#te baja todo el repo
git pull $URL	
git clone $URL	

#te agrega todos los cambios al staging
git add -A

#Adicionar archivo concreto
git add ${FILE}

#Para saber que hiciste
git commit -m "Comment" 

#Creo una branch nueva
git checkout -b dev

#Listo las branches
git branch

#Cambio entre Branches
git checkout main / dev

#Metes los cambios en el repo
git push origin ${BRANCH}

#Update my local branch
git pull

gitdir
git status
git add -A
git commit -m "UPDATE ...."
git push origin master

# https://code.mendhak.com/simple-bash-prompt-for-developers-ps1-git/
