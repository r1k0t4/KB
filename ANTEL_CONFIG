172.16.0.0/16 via 192.168.130.1 dev eth0    	## Zabbix, BKP, Jenkins, etc.	# MGMT

# DNSs
# 10.68.174.13
# 10.68.174.14

echo quit |telnet 10.68.174.13 53
echo quit |telnet 10.68.174.14 53

# MTA
# mta.stp.prod.vpc.sva.antel.com.uy

echo quit |telnet mta.stp.prod.vpc.sva.antel.com.uy 25

# NTP
#ntpq -pn
#ntpq -p
#/etc/ntp.conf
#/etc/chrony.conf

nsin01.stp.prod.mgmt.sva.antel.com.uy
nsin02.stp.prod.mgmt.sva.antel.com.uy

# ZABBIX - VIA MGMT
# ServerActive=172.16.2.42:10051

grep ^ServerActive /etc/zabbix/zabbix_agentd.conf
echo quit | telnet 172.16.2.42 10051

# PROXY [ 10.68.172.16:3128 ]
# proxy.stp.prod.vpc.sva.antel.com.uy:3128

echo quit |telnet proxy.stp.prod.vpc.sva.antel.com.uy 3128

# RHSM
# proxy_hostname = proxy.stp.prod.vpc.sva.antel.com.uy
# proxy_port = 3128

grep -E proxy_port\|hostname /etc/rhsm/rhsm.conf

# YUM
# proxy=http://proxy.stp.prod.vpc.sva.antel.com.uy:3128

grep -i proxy /etc/yum.conf 


